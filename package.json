{
  "name": "asset_map_app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:pg:generate": "drizzle-kit generate --config=drizzle.config.postgresql.ts",
    "db:pg:push": "drizzle-kit push --config=drizzle.config.postgresql.ts",
    "db:pg:pull": "drizzle-kit pull --config=drizzle.config.postgresql.ts",
    "db:pg:studio": "drizzle-kit studio --config=drizzle.config.postgresql.ts"
  },
  "dependencies": {
    "@expo/config-plugins": "~10.1.1",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.0.3",
    "@rnmapbox/maps": "^10.1.39",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.80.7",
    "@turf/helpers": "^7.2.0",
    "babel-plugin-inline-import": "^3.0.0",
    "base64-arraybuffer": "^1.0.2",
    "blob-util": "^2.0.2",
    "drizzle-orm": "^0.44.4",
    "expo": "53.0.20",
    "expo-checkbox": "^4.1.4",
    "expo-constants": "~17.1.4",
    "expo-crypto": "^15.0.7",
    "expo-drizzle-studio-plugin": "^0.2.0",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-router": "~5.1.4",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "formik": "^2.4.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "uuid": "^11.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.15.5",
    "@types/react": "~19.0.10",
    "ajv": "^8.12.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.25.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.2.5",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|uuid|@supabase|@rnmapbox))"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1",
      "^db/(.*)$": "<rootDir>/db/$1",
      "^drizzle/(.*)$": "<rootDir>/drizzle/$1"
    }
  },
  "private": true
}
